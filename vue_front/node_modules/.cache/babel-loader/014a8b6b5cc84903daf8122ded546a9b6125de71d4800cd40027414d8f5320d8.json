{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      value: [100, 500, 1000, 500, 3000, 5000],\n      rolLength: 6,\n      probabilities: [0.5, 0.15, 0.1, 0.15, 0.07, 0.03],\n      setNum: null,\n      modalMessage: \"\",\n      selectedValue: null\n    };\n  },\n  methods: {\n    rRandom() {\n      var rand = Math.random();\n      var sum = 0;\n      for (var i = 0; i < this.probabilities.length; i++) {\n        sum += this.probabilities[i];\n        if (rand < sum) return i;\n      }\n      return this.probabilities.length - 1;\n    },\n    rRotate() {\n      var panel = document.querySelector(\".rouletter-wacu\");\n      var btn = document.querySelector(\".rouletter-btn\");\n      var deg = [];\n      for (var i = 1, len = this.rolLength; i <= len; i++) {\n        deg.push(360 / len * i);\n      }\n      var num = 0;\n      this.$el.append(this.$refs.hiddenInput);\n      this.setNum = this.$refs.hiddenInput.value = this.rRandom();\n      this.modalMessage = \"\";\n      var ani = setInterval(() => {\n        num++;\n        panel.style.transform = \"rotate(\" + 360 * num + \"deg)\";\n        btn.disabled = true;\n        btn.style.pointerEvents = \"none\";\n        if (num === 50) {\n          clearInterval(ani);\n          panel.style.transform = `rotate(${deg[this.setNum]}deg)`;\n          console.log(this.selectedValue);\n          console.log(this.value[this.setNum]);\n          this.selectedValue = this.value[this.setNum + 1];\n          setTimeout(() => {\n            this.rLayerPopup(this.setNum);\n            btn.disabled = false;\n            btn.style.pointerEvents = \"auto\";\n          }, 5500);\n        }\n      }, 50);\n    },\n    rLayerPopup(num) {\n      const prizes = [100, 500, 1000, 500, 3000, 5000];\n      this.modalMessage = prizes[num];\n      var modal = this.$el.querySelector(\"#resultModal\");\n      modal.style.display = \"block\";\n    },\n    showModal(message) {\n      this.modalMessage = message;\n      var modal = this.$el.querySelector(\"#resultModal\");\n      modal.style.display = \"block\";\n    },\n    hideModal() {\n      var modal = this.$el.querySelector(\"#resultModal\");\n      modal.style.display = \"none\";\n    },\n    rReset() {\n      setTimeout(() => {\n        var btn = this.$el.querySelector(\".rouletter-btn\");\n        btn.disabled = false;\n        btn.style.pointerEvents = \"auto\";\n        this.rLayerPopup(this.setNum);\n        this.$refs.hiddenInput.remove();\n      }, 8000);\n    },\n    startRoulette() {\n      // 룰렛 값을 선택하는 로직\n      this.selectedValue = this.value[this.setNum];\n      console.log(this.setNum);\n      console.log(this.value[this.setNum]);\n      console.log(this.selectedValue);\n      this.rRotate();\n      this.rReset();\n\n      // 선택된 값을 바로 백엔드로 전송\n      const backServer = \"http://localhost:3000/roulette\";\n      axios.post(`${backServer}/addroul`, {\n        userEmail: \"user8@example.com\",\n        selectedValue: this.selectedValue\n      }).then(response => {\n        console.log(response.data.message);\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","value","rolLength","probabilities","setNum","modalMessage","selectedValue","methods","rRandom","rand","Math","random","sum","i","length","rRotate","panel","document","querySelector","btn","deg","len","push","num","$el","append","$refs","hiddenInput","ani","setInterval","style","transform","disabled","pointerEvents","clearInterval","console","log","setTimeout","rLayerPopup","prizes","modal","display","showModal","message","hideModal","rReset","remove","startRoulette","backServer","post","userEmail","then","response","catch","error"],"sources":["C:\\Users\\Lim\\Desktop\\BOOK_0728\\MOONMOON\\vue_front\\src\\views\\event.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <div class=\"rouletter\">\r\n      <div class=\"rouletter-bg\">\r\n        <div class=\"rouletter-wacu\"></div>\r\n      </div>\r\n      <div class=\"rouletter-arrow\"></div>\r\n      <button class=\"rouletter-btn\" @click=\"startRoulette()\">start</button>\r\n    </div>\r\n\r\n    <input type=\"hidden\" class=\"hidden-input\" ref=\"hiddenInput\" />\r\n    <div class=\"modal\" id=\"resultModal\">\r\n      <div class=\"modal-dialog\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-title\">축하합니다!</div>\r\n          <div class=\"modal-body\" id=\"modal-body\">\r\n            {{ selectedValue }}P 당첨되었습니다!\r\n          </div>\r\n          <div class=\"modal-footer\">\r\n            <span class=\"modal-close\" @click=\"hideModal\">&times;</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      value: [100, 500, 1000, 500, 3000, 5000],\r\n      rolLength: 6,\r\n      probabilities: [0.5, 0.15, 0.1, 0.15, 0.07, 0.03],\r\n      setNum: null,\r\n      modalMessage: \"\",\r\n      selectedValue: null,\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    rRandom() {\r\n      var rand = Math.random();\r\n      var sum = 0;\r\n      for (var i = 0; i < this.probabilities.length; i++) {\r\n        sum += this.probabilities[i];\r\n        if (rand < sum) return i;\r\n      }\r\n      return this.probabilities.length - 1;\r\n    },\r\n    rRotate() {\r\n      var panel = document.querySelector(\".rouletter-wacu\");\r\n      var btn = document.querySelector(\".rouletter-btn\");\r\n\r\n      var deg = [];\r\n      for (var i = 1, len = this.rolLength; i <= len; i++) {\r\n        deg.push((360 / len) * i);\r\n      }\r\n\r\n      var num = 0;\r\n\r\n      this.$el.append(this.$refs.hiddenInput);\r\n      this.setNum = this.$refs.hiddenInput.value = this.rRandom();\r\n      this.modalMessage = \"\";\r\n\r\n      var ani = setInterval(() => {\r\n        num++;\r\n        panel.style.transform = \"rotate(\" + 360 * num + \"deg)\";\r\n        btn.disabled = true;\r\n        btn.style.pointerEvents = \"none\";\r\n\r\n        if (num === 50) {\r\n          clearInterval(ani);\r\n          panel.style.transform = `rotate(${deg[this.setNum]}deg)`;\r\n          console.log(this.selectedValue);\r\n          console.log(this.value[this.setNum]);\r\n\r\n          this.selectedValue = this.value[this.setNum + 1];\r\n\r\n          setTimeout(() => {\r\n            this.rLayerPopup(this.setNum);\r\n            btn.disabled = false;\r\n            btn.style.pointerEvents = \"auto\";\r\n          }, 5500);\r\n        }\r\n      }, 50);\r\n    },\r\n    rLayerPopup(num) {\r\n      const prizes = [100, 500, 1000, 500, 3000, 5000];\r\n\r\n      this.modalMessage = prizes[num];\r\n      var modal = this.$el.querySelector(\"#resultModal\");\r\n      modal.style.display = \"block\";\r\n    },\r\n    showModal(message) {\r\n      this.modalMessage = message;\r\n      var modal = this.$el.querySelector(\"#resultModal\");\r\n      modal.style.display = \"block\";\r\n    },\r\n    hideModal() {\r\n      var modal = this.$el.querySelector(\"#resultModal\");\r\n      modal.style.display = \"none\";\r\n    },\r\n    rReset() {\r\n      setTimeout(() => {\r\n        var btn = this.$el.querySelector(\".rouletter-btn\");\r\n        btn.disabled = false;\r\n        btn.style.pointerEvents = \"auto\";\r\n        this.rLayerPopup(this.setNum);\r\n        this.$refs.hiddenInput.remove();\r\n      }, 8000);\r\n    },\r\n    startRoulette() {\r\n      // 룰렛 값을 선택하는 로직\r\n      this.selectedValue = this.value[this.setNum];\r\n      console.log(this.setNum);\r\n      console.log(this.value[this.setNum]);\r\n      console.log(this.selectedValue);\r\n      this.rRotate();\r\n      this.rReset();\r\n\r\n      // 선택된 값을 바로 백엔드로 전송\r\n      const backServer = \"http://localhost:3000/roulette\";\r\n      axios\r\n        .post(`${backServer}/addroul`, {\r\n          userEmail: \"user8@example.com\",\r\n          selectedValue: this.selectedValue,\r\n        })\r\n        .then((response) => {\r\n          console.log(response.data.message);\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.rouletter {\r\n  position: relative;\r\n  width: 400px;\r\n  height: 400px;\r\n}\r\n.rouletter-bg {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  width: 350px;\r\n  height: 350px;\r\n  border-radius: 350px;\r\n  overflow: hidden;\r\n\r\n  border: 25px solid rgb(127, 127, 170);\r\n}\r\n.rouletter-wacu {\r\n  width: 100%;\r\n  height: 100%;\r\n  background: #f5f5f2 url(\"../assets/룰.png\") no-repeat;\r\n  background-size: 100%;\r\n  transform-origin: center;\r\n  transition-timing-function: ease-in-out;\r\n  transition: 5s;\r\n}\r\n\r\n.rouletter-arrow {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n  width: 1px;\r\n  height: 1px;\r\n  border-right: 10px solid transparent;\r\n  border-left: 10px solid transparent;\r\n  border-top: 40px solid rgb(127, 127, 170);\r\n  border-bottom: 0px solid transparent;\r\n}\r\n.rouletter-btn {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  width: 80px;\r\n  height: 80px;\r\n  border-radius: 80px;\r\n  background: #fff;\r\n  border-image: linear-gradient(to right, #fbfcb9be, #ffcdf3aa, #65d3ffaa);\r\n  border: 2px solid;\r\n}\r\n.hidden-input {\r\n  display: none;\r\n}\r\n.modal {\r\n  display: none;\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n}\r\n.modal-dialog {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  width: 300px;\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  border-radius: 5px;\r\n}\r\n.modal-title {\r\n  font-weight: bold;\r\n  margin-bottom: 10px;\r\n}\r\n.modal-body {\r\n  margin-bottom: 20px;\r\n}\r\n.modal-footer {\r\n  text-align: right;\r\n}\r\n.modal-close {\r\n  cursor: pointer;\r\n  color: #999;\r\n}\r\n</style>\r\n"],"mappings":";AA4BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CAAC,GAAG,EAAE,GAAG,EAAE,IAAI,EAAE,GAAG,EAAE,IAAI,EAAE,IAAI,CAAC;MACxCC,SAAS,EAAE,CAAC;MACZC,aAAa,EAAE,CAAC,GAAG,EAAE,IAAI,EAAE,GAAG,EAAE,IAAI,EAAE,IAAI,EAAE,IAAI,CAAC;MACjDC,MAAM,EAAE,IAAI;MACZC,YAAY,EAAE,EAAE;MAChBC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,OAAOA,CAAA,EAAG;MACR,IAAIC,IAAG,GAAIC,IAAI,CAACC,MAAM,CAAC,CAAC;MACxB,IAAIC,GAAE,GAAI,CAAC;MACX,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAACV,aAAa,CAACW,MAAM,EAAED,CAAC,EAAE,EAAE;QAClDD,GAAE,IAAK,IAAI,CAACT,aAAa,CAACU,CAAC,CAAC;QAC5B,IAAIJ,IAAG,GAAIG,GAAG,EAAE,OAAOC,CAAC;MAC1B;MACA,OAAO,IAAI,CAACV,aAAa,CAACW,MAAK,GAAI,CAAC;IACtC,CAAC;IACDC,OAAOA,CAAA,EAAG;MACR,IAAIC,KAAI,GAAIC,QAAQ,CAACC,aAAa,CAAC,iBAAiB,CAAC;MACrD,IAAIC,GAAE,GAAIF,QAAQ,CAACC,aAAa,CAAC,gBAAgB,CAAC;MAElD,IAAIE,GAAE,GAAI,EAAE;MACZ,KAAK,IAAIP,CAAA,GAAI,CAAC,EAAEQ,GAAE,GAAI,IAAI,CAACnB,SAAS,EAAEW,CAAA,IAAKQ,GAAG,EAAER,CAAC,EAAE,EAAE;QACnDO,GAAG,CAACE,IAAI,CAAE,GAAE,GAAID,GAAG,GAAIR,CAAC,CAAC;MAC3B;MAEA,IAAIU,GAAE,GAAI,CAAC;MAEX,IAAI,CAACC,GAAG,CAACC,MAAM,CAAC,IAAI,CAACC,KAAK,CAACC,WAAW,CAAC;MACvC,IAAI,CAACvB,MAAK,GAAI,IAAI,CAACsB,KAAK,CAACC,WAAW,CAAC1B,KAAI,GAAI,IAAI,CAACO,OAAO,CAAC,CAAC;MAC3D,IAAI,CAACH,YAAW,GAAI,EAAE;MAEtB,IAAIuB,GAAE,GAAIC,WAAW,CAAC,MAAM;QAC1BN,GAAG,EAAE;QACLP,KAAK,CAACc,KAAK,CAACC,SAAQ,GAAI,SAAQ,GAAI,GAAE,GAAIR,GAAE,GAAI,MAAM;QACtDJ,GAAG,CAACa,QAAO,GAAI,IAAI;QACnBb,GAAG,CAACW,KAAK,CAACG,aAAY,GAAI,MAAM;QAEhC,IAAIV,GAAE,KAAM,EAAE,EAAE;UACdW,aAAa,CAACN,GAAG,CAAC;UAClBZ,KAAK,CAACc,KAAK,CAACC,SAAQ,GAAK,UAASX,GAAG,CAAC,IAAI,CAAChB,MAAM,CAAE,MAAK;UACxD+B,OAAO,CAACC,GAAG,CAAC,IAAI,CAAC9B,aAAa,CAAC;UAC/B6B,OAAO,CAACC,GAAG,CAAC,IAAI,CAACnC,KAAK,CAAC,IAAI,CAACG,MAAM,CAAC,CAAC;UAEpC,IAAI,CAACE,aAAY