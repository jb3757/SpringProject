 //세션스토리지에 데이터가 있거나 배열에 포함되어있으면\r\n        selectedTitle.value = tapName;\r\n        //선택 탭을 세션스토리지 데이터로 설정\r\n    } else {\r\n        selectedTitle.value = tabTitles.value[0];\r\n        //없으면 첫번째 탭으로 설정\r\n        sessionStorage.setItem(\"eventTap\", tabTitles.value[0]);\r\n        //세션스토리지 데이터도 다시 설정\r\n    }\r\n});\r\n\r\nconst handleTabClick = (title) => {\r\n    selectedTitle.value = title;\r\n    //탭 선택에 데이터 넣은뒤\r\n    sessionStorage.setItem(\"eventTap\", title);\r\n    // 선택한 탭을 sessionStorage에 저장\r\n};\r\n\r\nprovide(\"selectedTitle\", selectedTitle);\r\n</script>\r\n\r\n<template>\r\n    <div class=\"tabs\">\r\n        <ul class=\"tabs__header\">\r\n            <li\r\n                v-for=\"title in tabTitles\"\r\n                :key=\"title\"\r\n                class=\"tabs__item\"\r\n                :class=\"{ selected: selectedTitle === title }\"\r\n                @click=\"handleTabClick(title)\"\r\n            >\r\n                {{ title }}\r\n            </li>\r\n        </ul>\r\n\r\n        <slot />\r\n    </div>\r\n</template>\r\n"],"mappings":";;EAmCSA,KAAK,EAAC;AAAM;;EACTA,KAAK,EAAC;AAAc;;;uBAD5BC,mBAAA,CAcM,OAdNC,UAcM,GAbFC,mBAAA,CAUK,MAVLC,UAUK,I,kBATDH,mBAAA,CAQKI,SAAA,QAAAC,WAAA,CAPeC,MAAA,CAAAC,SAAS,EAAlBC,KAAK;yBADhBR,mBAAA,CAQK;MANAS,GAAG,EAAED,KAAK;MACXT,KAAK,EAAAW,eAAA,EAAC,YAAY;QAAAC,QAAA,EACEL,MAAA,CAAAM,aAAa,KAAKJ;MAAK;MAC1CK,OAAK,EAAAC,MAAA,IAAER,MAAA,CAAAS,cAAc,CAACP,KAAK;wBAEzBA,KAAK,gCAAAQ,UAAA;oCAIhBC,WAAA,CAAQC,IAAA,CAAAC,MAAA,a"},"metadata":{},"sourceType":"module","externalDependencies":[]}