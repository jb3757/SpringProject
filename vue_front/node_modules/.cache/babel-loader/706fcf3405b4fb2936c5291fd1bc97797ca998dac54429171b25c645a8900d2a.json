{"ast":null,"code":"import axios from \"axios\";\nimport { Pagination, Navigation } from \"vue3-carousel\";\nexport default {\n  name: \"\",\n  components: {\n    Pagination,\n    Navigation\n  },\n  data() {\n    return {\n      getUserData: {\n        USER_NAME: \"\",\n        USER_EMAIL: \"\",\n        USER_POINT: \"\"\n      },\n      likeCountData: 0,\n      recentCountData: 0,\n      getCouCountData: 0,\n      recListData: [],\n      couponGrade: \"\",\n      nextGrade: \"\"\n    };\n  },\n  created() {\n    this.email = localStorage.getItem(\"userID\");\n    this.getUser();\n    this.getLikeCount();\n    this.getRecentCount();\n    this.getRecList();\n    this.getCouCount();\n    this.getGraInfo();\n    this.getGraUp();\n  },\n  methods: {\n    getUser() {\n      axios({\n        url: \"http://localhost:3000/mypage/getuser\",\n        method: \"post\",\n        data: {\n          userEmail: this.email\n        }\n      }).then(response => {\n        this.getUserData = response.data.user;\n      }).catch(error => {\n        console.error(\"user데이터 받아오기 오류:\", error);\n      });\n    },\n    async getLikeCount() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/mypage/getlikecount\", {\n          params: {\n            userEmail: this.email\n          }\n        });\n\n        // console.log(\"서버 응답 데이터:\", response.data.likecount);\n        this.likeCountData = response.data.likecount; // 할당\n      } catch (error) {\n        console.error(\"userlikecount 데이터 받아오기 오류:\", error);\n      }\n    },\n    async getRecentCount() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/mypage/recentcount\", {\n          params: {\n            userEmail: this.email\n          }\n        });\n\n        // console.log(\"서버 응답 데이터:\", response.data.recentCount);\n        this.recentCountData = response.data.recentCount; // 할당\n      } catch (error) {\n        console.error(\"userlikecount 데이터 받아오기 오류:\", error);\n      }\n    },\n    async getCouCount() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/mypage/couponcount\", {\n          params: {\n            userEmail: this.email\n          }\n        });\n        this.getCouCountData = response.data.couponCount;\n        // console.log(response.data.couponCount);\n      } catch (error) {\n        console.error(\"쿠폰카운트 데이터 받아오기 오류:\", error);\n      }\n    },\n    async getRecList() {\n      if (localStorage.getItem(\"userID\")) {\n        try {\n          const response = await axios.post(\"http://localhost:3000/booklist/getRecList\", {\n            email: this.email\n          });\n          this.recListData = response.data;\n          // console.log(response.data);\n          // displayedPosts에 categoryData 데이터 복사\n        } catch (error) {\n          alert(error);\n          console.error(error);\n        }\n      }\n    },\n    getGraInfo() {\n      const userEmail = localStorage.getItem(\"userID\");\n      axios({\n        url: `/mypage/coupongrade/${userEmail}`,\n        method: \"GET\"\n      }).then(res => {\n        console.log(res.data);\n        this.couponGrade = res.data.userGrade;\n        this.setNextGra();\n      });\n    },\n    getGraUp() {\n      const userEmail = localStorage.getItem(\"userID\");\n      axios({\n        url: `/mypage/couponupgrade/${userEmail}`,\n        method: \"GET\"\n      }).then(res => {\n        console.log(res.data);\n      });\n    },\n    setNextGra() {\n      if (this.couponGrade === \"프\") {\n        this.nextGrade = \"골드\";\n        return;\n      } else {}\n    }\n  },\n  computed: {}\n};","map":{"version":3,"names":["axios","Pagination","Navigation","name","components","data","getUserData","USER_NAME","USER_EMAIL","USER_POINT","likeCountData","recentCountData","getCouCountData","recListData","couponGrade","nextGrade","created","email","localStorage","getItem","getUser","getLikeCount","getRecentCount","getRecList","getCouCount","getGraInfo","getGraUp","methods","url","method","userEmail","then","response","user","catch","error","console","get","params","likecount","recentCount","couponCount","post","alert","res","log","userGrade","setNextGra","computed"],"sources":["C:\\Users\\Lim\\Desktop\\BOOK_0728\\MOONMOON\\vue_front\\src\\components\\myPage_top_grade.vue"],"sourcesContent":["<template>\r\n    <div class=\"mypage_top\">\r\n        <div class=\"mypage_top_left\">\r\n            <div class=\"mypage_profile_name_email\">\r\n                <div class=\"mypage_profile\">\r\n                    <img src=\"../assets/img/user.png\" alt=\"user\" />\r\n                </div>\r\n                <div class=\"mypage_profile_right\">\r\n                    <div class=\"name_grade\">\r\n                        <div class=\"mypage_grade\">{{ couponGrade }}</div>\r\n                        <div class=\"mypage_name\">{{ getUserData.USER_NAME }}<span>님</span></div>\r\n                    </div>\r\n                    <div class=\"mypage_email\">{{ getUserData.USER_EMAIL }}</div>\r\n                </div>\r\n            </div>\r\n            <div class=\"like_recent_point_cou\">\r\n                <a href=\"#\" class=\"mypage_like\">\r\n                    <div class=\"like_recent_point_cou_text\">\r\n                        <div class=\"like_recent_point_cou_title\">찜</div>\r\n                        <div class=\"like_recent_point_cou_num\">{{ likeCountData }}</div>\r\n                    </div></a\r\n                >\r\n                <a href=\"#\" class=\"mypage_like\">\r\n                    <div class=\"like_recent_point_cou_text\">\r\n                        <div class=\"like_recent_point_cou_title\">최근 본</div>\r\n                        <div class=\"like_recent_point_cou_num\">{{ recentCountData }}</div>\r\n                    </div></a\r\n                >\r\n                <a href=\"#\" class=\"mypage_like\">\r\n                    <div class=\"like_recent_point_cou_text\">\r\n                        <div class=\"like_recent_point_cou_title\">포인트</div>\r\n                        <div class=\"like_recent_point_cou_num\">{{ getUserData.USER_POINT }}<span>p</span></div>\r\n                    </div></a\r\n                >\r\n                <a href=\"#\" class=\"mypage_like\">\r\n                    <div class=\"like_recent_point_cou_text\">\r\n                        <div class=\"like_recent_point_cou_title\">쿠폰</div>\r\n                        <div class=\"like_recent_point_cou_num\">{{ getCouCountData }}</div>\r\n                    </div></a\r\n                >\r\n            </div>\r\n        </div>\r\n        <div class=\"mypage_top_second_right\">\r\n            <div class=\"grade_info_wrap\">\r\n                <div class=\"grade_info_top\">\r\n                    <div class=\"next_grade\">다음달 예정 등급 :</div>\r\n                    <div class=\"next_grade_db\">db용</div>\r\n                </div>\r\n                <div class=\"grade_info_second\">\r\n                    <div class=\"next_grade_s\">db용</div>\r\n                    <div class=\"next_grade_t\">{{ nextGrade }} 등급 혜택을 받으려면?</div>\r\n                </div>\r\n                <div class=\"grade_info_under\">\r\n                    <div class=\"next_grade_u\">추가 구매금액 :</div>\r\n                    <div class=\"ext_grade_u2\">db용 원</div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { Pagination, Navigation } from \"vue3-carousel\";\r\n\r\nexport default {\r\n    name: \"\",\r\n    components: {\r\n        Pagination,\r\n        Navigation,\r\n    },\r\n    data() {\r\n        return {\r\n            getUserData: {\r\n                USER_NAME: \"\",\r\n                USER_EMAIL: \"\",\r\n                USER_POINT: \"\",\r\n            },\r\n            likeCountData: 0,\r\n            recentCountData: 0,\r\n            getCouCountData: 0,\r\n            recListData: [],\r\n            couponGrade: \"\",\r\n            nextGrade: \"\",\r\n        };\r\n    },\r\n    created() {\r\n        this.email = localStorage.getItem(\"userID\");\r\n        this.getUser();\r\n        this.getLikeCount();\r\n        this.getRecentCount();\r\n        this.getRecList();\r\n        this.getCouCount();\r\n        this.getGraInfo();\r\n        this.getGraUp();\r\n    },\r\n    methods: {\r\n        getUser() {\r\n            axios({\r\n                url: \"http://localhost:3000/mypage/getuser\",\r\n                method: \"post\",\r\n                data: {\r\n                    userEmail: this.email,\r\n                },\r\n            })\r\n                .then((response) => {\r\n                    this.getUserData = response.data.user;\r\n                })\r\n                .catch((error) => {\r\n                    console.error(\"user데이터 받아오기 오류:\", error);\r\n                });\r\n        },\r\n        async getLikeCount() {\r\n            try {\r\n                const response = await axios.get(\"http://localhost:3000/mypage/getlikecount\", {\r\n                    params: {\r\n                        userEmail: this.email,\r\n                    },\r\n                });\r\n\r\n                // console.log(\"서버 응답 데이터:\", response.data.likecount);\r\n                this.likeCountData = response.data.likecount; // 할당\r\n            } catch (error) {\r\n                console.error(\"userlikecount 데이터 받아오기 오류:\", error);\r\n            }\r\n        },\r\n        async getRecentCount() {\r\n            try {\r\n                const response = await axios.get(\"http://localhost:3000/mypage/recentcount\", {\r\n                    params: {\r\n                        userEmail: this.email,\r\n                    },\r\n                });\r\n\r\n                // console.log(\"서버 응답 데이터:\", response.data.recentCount);\r\n                this.recentCountData = response.data.recentCount; // 할당\r\n            } catch (error) {\r\n                console.error(\"userlikecount 데이터 받아오기 오류:\", error);\r\n            }\r\n        },\r\n        async getCouCount() {\r\n            try {\r\n                const response = await axios.get(\"http://localhost:3000/mypage/couponcount\", {\r\n                    params: {\r\n                        userEmail: this.email,\r\n                    },\r\n                });\r\n                this.getCouCountData = response.data.couponCount;\r\n                // console.log(response.data.couponCount);\r\n            } catch (error) {\r\n                console.error(\"쿠폰카운트 데이터 받아오기 오류:\", error);\r\n            }\r\n        },\r\n        async getRecList() {\r\n            if (localStorage.getItem(\"userID\")) {\r\n                try {\r\n                    const response = await axios.post(\"http://localhost:3000/booklist/getRecList\", {\r\n                        email: this.email,\r\n                    });\r\n\r\n                    this.recListData = response.data;\r\n                    // console.log(response.data);\r\n                    // displayedPosts에 categoryData 데이터 복사\r\n                } catch (error) {\r\n                    alert(error);\r\n                    console.error(error);\r\n                }\r\n            }\r\n        },\r\n        getGraInfo() {\r\n            const userEmail = localStorage.getItem(\"userID\");\r\n\r\n            axios({\r\n                url: `/mypage/coupongrade/${userEmail}`,\r\n                method: \"GET\",\r\n            }).then((res) => {\r\n                console.log(res.data);\r\n                this.couponGrade = res.data.userGrade;\r\n\r\n                this.setNextGra();\r\n            });\r\n        },\r\n        getGraUp() {\r\n            const userEmail = localStorage.getItem(\"userID\");\r\n\r\n            axios({\r\n                url: `/mypage/couponupgrade/${userEmail}`,\r\n                method: \"GET\",\r\n            }).then((res) => {\r\n                console.log(res.data);\r\n            });\r\n        },\r\n        setNextGra() {\r\n            if (this.couponGrade === \"프\") {\r\n                this.nextGrade = \"골드\";\r\n                return;\r\n            } else {\r\n            }\r\n        },\r\n    },\r\n    computed: {},\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.mypage_top_second_right {\r\n    width: 100%;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n}\r\n.grade_info_wrap {\r\n    border: 2px solid red;\r\n    width: 100%;\r\n    height: 75%;\r\n    margin: 4% 4% 6% 4%;\r\n    background-color: white;\r\n}\r\n.grade_info_top {\r\n    display: flex;\r\n    float: right;\r\n    margin: 5% 5% 0 0;\r\n}\r\n.next_grade {\r\n    font-size: large;\r\n    color: #a0a0a0;\r\n}\r\n</style>\r\n"],"mappings":"AA8DA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAU,EAAEC,UAAS,QAAS,eAAe;AAEtD,eAAe;EACXC,IAAI,EAAE,EAAE;EACRC,UAAU,EAAE;IACRH,UAAU;IACVC;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,WAAW,EAAE;QACTC,SAAS,EAAE,EAAE;QACbC,UAAU,EAAE,EAAE;QACdC,UAAU,EAAE;MAChB,CAAC;MACDC,aAAa,EAAE,CAAC;MAChBC,eAAe,EAAE,CAAC;MAClBC,eAAe,EAAE,CAAC;MAClBC,WAAW,EAAE,EAAE;MACfC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE;IACf,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC;IAC3C,IAAI,CAACC,OAAO,CAAC,CAAC;IACd,IAAI,CAACC,YAAY,CAAC,CAAC;IACnB,IAAI,CAACC,cAAc,CAAC,CAAC;IACrB,IAAI,CAACC,UAAU,CAAC,CAAC;IACjB,IAAI,CAACC,WAAW,CAAC,CAAC;IAClB,IAAI,CAACC,UAAU,CAAC,CAAC;IACjB,IAAI,CAACC,QAAQ,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACLP,OAAOA,CAAA,EAAG;MACNpB,KAAK,CAAC;QACF4B,GAAG,EAAE,sCAAsC;QAC3CC,MAAM,EAAE,MAAM;QACdxB,IAAI,EAAE;UACFyB,SAAS,EAAE,IAAI,CAACb;QACpB;MACJ,CAAC,EACIc,IAAI,CAAEC,QAAQ,IAAK;QAChB,IAAI,CAAC1B,WAAU,GAAI0B,QAAQ,CAAC3B,IAAI,CAAC4B,IAAI;MACzC,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,kBAAkB,EAAEA,KAAK,CAAC;MAC5C,CAAC,CAAC;IACV,CAAC;IACD,MAAMd,YAAYA,CAAA,EAAG;MACjB,IAAI;QACA,MAAMW,QAAO,GAAI,MAAMhC,KAAK,CAACqC,GAAG,CAAC,2CAA2C,EAAE;UAC1EC,MAAM,EAAE;YACJR,SAAS,EAAE,IAAI,CAACb;UACpB;QACJ,CAAC,