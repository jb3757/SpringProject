{"ast":null,"code":"import axios from \"axios\";\nimport { Carousel, Slide, Pagination, Navigation } from \"vue3-carousel\";\nexport default {\n  name: \"\",\n  components: {\n    Carousel,\n    Slide,\n    Pagination,\n    Navigation\n  },\n  data() {\n    return {\n      getUserData: {\n        USER_NAME: \"\",\n        USER_EMAIL: \"\",\n        USER_POINT: \"\"\n      },\n      likeCountData: 0,\n      recentCountData: 0,\n      getCouCountData: 0,\n      recListData: [],\n      couponGrade: \"\"\n    };\n  },\n  created() {\n    this.email = localStorage.getItem(\"userID\");\n    this.getUser();\n    this.getLikeCount();\n    this.getRecentCount();\n    this.getRecList();\n    this.getCouCount();\n  },\n  methods: {\n    getGraInfo() {\n      const userEmail = localStorage.getItem(\"userID\");\n      axios({\n        url: `/mypage/coupongrade/${userEmail}`,\n        method: \"GET\"\n      }).then(res => {\n        console.log(res.data);\n        this.couponGrade = res.data.userGrade;\n        this.setNextGra();\n      });\n    },\n    getUser() {\n      axios({\n        url: \"http://localhost:3000/mypage/getuser\",\n        method: \"post\",\n        data: {\n          userEmail: this.email\n        }\n      }).then(response => {\n        this.getUserData = response.data.user;\n      }).catch(error => {\n        console.error(\"user데이터 받아오기 오류:\", error);\n      });\n    },\n    async getLikeCount() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/mypage/getlikecount\", {\n          params: {\n            userEmail: this.email\n          }\n        });\n\n        // console.log(\"서버 응답 데이터:\", response.data.likecount);\n        this.likeCountData = response.data.likecount; // 할당\n      } catch (error) {\n        console.error(\"userlikecount 데이터 받아오기 오류:\", error);\n      }\n    },\n    async getRecentCount() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/mypage/recentcount\", {\n          params: {\n            userEmail: this.email\n          }\n        });\n\n        // console.log(\"서버 응답 데이터:\", response.data.recentCount);\n        this.recentCountData = response.data.recentCount; // 할당\n      } catch (error) {\n        console.error(\"userlikecount 데이터 받아오기 오류:\", error);\n      }\n    },\n    async getCouCount() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/mypage/couponcount\", {\n          params: {\n            userEmail: this.email\n          }\n        });\n        this.getCouCountData = response.data.couponCount;\n        // console.log(response.data.couponCount);\n      } catch (error) {\n        console.error(\"쿠폰카운트 데이터 받아오기 오류:\", error);\n      }\n    },\n    async getRecList() {\n      if (localStorage.getItem(\"userID\")) {\n        try {\n          const response = await axios.post(\"http://localhost:3000/booklist/getRecList\", {\n            email: this.email\n          });\n          this.recListData = response.data;\n          // console.log(response.data);\n          // displayedPosts에 categoryData 데이터 복사\n        } catch (error) {\n          alert(error);\n          console.error(error);\n        }\n      }\n    },\n    formatNumber(number) {\n      // 숫자를 천 단위마다 쉼표가 있는 형식으로 변환\n      return new Intl.NumberFormat().format(number);\n    }\n  }\n};","map":{"version":3,"names":["axios","Carousel","Slide","Pagination","Navigation","name","components","data","getUserData","USER_NAME","USER_EMAIL","USER_POINT","likeCountData","recentCountData","getCouCountData","recListData","couponGrade","created","email","localStorage","getItem","getUser","getLikeCount","getRecentCount","getRecList","getCouCount","methods","getGraInfo","userEmail","url","method","then","res","console","log","userGrade","setNextGra","response","user","catch","error","get","params","likecount","recentCount","couponCount","post","alert","formatNumber","number","Intl","NumberFormat","format"],"sources":["C:\\Users\\Lim\\Desktop\\BOOK_0728\\MOONMOON\\vue_front\\src\\components\\myPage_top.vue"],"sourcesContent":["<template>\r\n    <div class=\"mypage_top\">\r\n        <div class=\"mypage_top_left\">\r\n            <div class=\"mypage_profile_name_email\">\r\n                <div class=\"mypage_profile\">\r\n                    <img src=\"../assets/img/user.png\" alt=\"user\" />\r\n                </div>\r\n                <div class=\"mypage_profile_right\">\r\n                    <div class=\"name_grade\">\r\n                        <div class=\"mypage_grade\">{{ couponGrade }}</div>\r\n                        <div class=\"mypage_name\">{{ getUserData.USER_NAME }}<span>님</span></div>\r\n                    </div>\r\n                    <div class=\"mypage_email\">{{ getUserData.USER_EMAIL }}</div>\r\n                </div>\r\n            </div>\r\n            <div class=\"like_recent_point_cou\">\r\n                <a href=\"#\" class=\"mypage_like\">\r\n                    <div class=\"like_recent_point_cou_text\">\r\n                        <div class=\"like_recent_point_cou_title\">찜</div>\r\n                        <div class=\"like_recent_point_cou_num\">{{ likeCountData }}</div>\r\n                    </div></a\r\n                >\r\n                <a href=\"#\" class=\"mypage_like\">\r\n                    <div class=\"like_recent_point_cou_text\">\r\n                        <div class=\"like_recent_point_cou_title\">최근 본</div>\r\n                        <div class=\"like_recent_point_cou_num\">{{ recentCountData }}</div>\r\n                    </div></a\r\n                >\r\n                <a href=\"#\" class=\"mypage_like\">\r\n                    <div class=\"like_recent_point_cou_text\">\r\n                        <div class=\"like_recent_point_cou_title\">포인트</div>\r\n                        <div class=\"like_recent_point_cou_num\">{{ formatNumber(getUserData.USER_POINT) }}<span>p</span></div>\r\n                    </div></a\r\n                >\r\n                <a href=\"#\" class=\"mypage_like\">\r\n                    <div class=\"like_recent_point_cou_text\">\r\n                        <div class=\"like_recent_point_cou_title\">쿠폰</div>\r\n                        <div class=\"like_recent_point_cou_num\">{{ formatNumber(getCouCountData) }}</div>\r\n                    </div></a\r\n                >\r\n            </div>\r\n        </div>\r\n        <div class=\"mypage_top_right\">\r\n            <div class=\"mypage_slide_title\">신간 추천</div>\r\n            <carousel :items-to-show=\"4\" class=\"mypage_slide_set\">\r\n                <template #addons>\r\n                    <navigation />\r\n                    <pagination />\r\n                </template>\r\n                <slide v-for=\"(item, index) in recListData.slice(0, 9)\" :key=\"index\" class=\"mypage_slide\">\r\n                    <div class=\"slide_content\">\r\n                        <!-- <div class=\"my_rec_slide_title\">{{ item.BOOK_TITLE }}</div> -->\r\n                        <div class=\"my_rec_slide_img\"><img :src=\"item.BOOK_COVER\" :alt=\"item.BOOK_TITLE\" /></div>\r\n                    </div>\r\n                </slide>\r\n            </carousel>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { Carousel, Slide, Pagination, Navigation } from \"vue3-carousel\";\r\n\r\nexport default {\r\n    name: \"\",\r\n    components: {\r\n        Carousel,\r\n        Slide,\r\n        Pagination,\r\n        Navigation,\r\n    },\r\n    data() {\r\n        return {\r\n            getUserData: {\r\n                USER_NAME: \"\",\r\n                USER_EMAIL: \"\",\r\n                USER_POINT: \"\",\r\n            },\r\n            likeCountData: 0,\r\n            recentCountData: 0,\r\n            getCouCountData: 0,\r\n            recListData: [],\r\n            couponGrade: \"\",\r\n        };\r\n    },\r\n    created() {\r\n        this.email = localStorage.getItem(\"userID\");\r\n        this.getUser();\r\n        this.getLikeCount();\r\n        this.getRecentCount();\r\n        this.getRecList();\r\n        this.getCouCount();\r\n    },\r\n    methods: {\r\n        getGraInfo() {\r\n            const userEmail = localStorage.getItem(\"userID\");\r\n\r\n            axios({\r\n                url: `/mypage/coupongrade/${userEmail}`,\r\n                method: \"GET\",\r\n            }).then((res) => {\r\n                console.log(res.data);\r\n                this.couponGrade = res.data.userGrade;\r\n\r\n                this.setNextGra();\r\n            });\r\n        },\r\n        getUser() {\r\n            axios({\r\n                url: \"http://localhost:3000/mypage/getuser\",\r\n                method: \"post\",\r\n                data: {\r\n                    userEmail: this.email,\r\n                },\r\n            })\r\n                .then((response) => {\r\n                    this.getUserData = response.data.user;\r\n                })\r\n                .catch((error) => {\r\n                    console.error(\"user데이터 받아오기 오류:\", error);\r\n                });\r\n        },\r\n        async getLikeCount() {\r\n            try {\r\n                const response = await axios.get(\"http://localhost:3000/mypage/getlikecount\", {\r\n                    params: {\r\n                        userEmail: this.email,\r\n                    },\r\n                });\r\n\r\n                // console.log(\"서버 응답 데이터:\", response.data.likecount);\r\n                this.likeCountData = response.data.likecount; // 할당\r\n            } catch (error) {\r\n                console.error(\"userlikecount 데이터 받아오기 오류:\", error);\r\n            }\r\n        },\r\n        async getRecentCount() {\r\n            try {\r\n                const response = await axios.get(\"http://localhost:3000/mypage/recentcount\", {\r\n                    params: {\r\n                        userEmail: this.email,\r\n                    },\r\n                });\r\n\r\n                // console.log(\"서버 응답 데이터:\", response.data.recentCount);\r\n                this.recentCountData = response.data.recentCount; // 할당\r\n            } catch (error) {\r\n                console.error(\"userlikecount 데이터 받아오기 오류:\", error);\r\n            }\r\n        },\r\n        async getCouCount() {\r\n            try {\r\n                const response = await axios.get(\"http://localhost:3000/mypage/couponcount\", {\r\n                    params: {\r\n                        userEmail: this.email,\r\n                    },\r\n                });\r\n                this.getCouCountData = response.data.couponCount;\r\n                // console.log(response.data.couponCount);\r\n            } catch (error) {\r\n                console.error(\"쿠폰카운트 데이터 받아오기 오류:\", error);\r\n            }\r\n        },\r\n        async getRecList() {\r\n            if (localStorage.getItem(\"userID\")) {\r\n                try {\r\n                    const response = await axios.post(\"http://localhost:3000/booklist/getRecList\", {\r\n                        email: this.email,\r\n                    });\r\n\r\n                    this.recListData = response.data;\r\n                    // console.log(response.data);\r\n                    // displayedPosts에 categoryData 데이터 복사\r\n                } catch (error) {\r\n                    alert(error);\r\n                    console.error(error);\r\n                }\r\n            }\r\n        },\r\n        formatNumber(number) {\r\n            // 숫자를 천 단위마다 쉼표가 있는 형식으로 변환\r\n            return new Intl.NumberFormat().format(number);\r\n        },\r\n    },\r\n};\r\n</script>\r\n"],"mappings":"AA6DA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAQ,EAAEC,KAAK,EAAEC,UAAU,EAAEC,UAAS,QAAS,eAAe;AAEvE,eAAe;EACXC,IAAI,EAAE,EAAE;EACRC,UAAU,EAAE;IACRL,QAAQ;IACRC,KAAK;IACLC,UAAU;IACVC;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,WAAW,EAAE;QACTC,SAAS,EAAE,EAAE;QACbC,UAAU,EAAE,EAAE;QACdC,UAAU,EAAE;MAChB,CAAC;MACDC,aAAa,EAAE,CAAC;MAChBC,eAAe,EAAE,CAAC;MAClBC,eA