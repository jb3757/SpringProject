{"ast":null,"code":"import axios from 'axios';\nimport GnbBar from '../components/gnbBar.vue';\nexport default {\n  components: {\n    GnbBar\n  },\n  data() {\n    return {\n      qna: {\n        email: \"\",\n        qna_title: \"\",\n        qna_con: \"\",\n        qna_rep: 0\n      },\n      qna_id: \"\"\n    };\n  },\n  created() {\n    this.qna.email = localStorage.getItem(\"userID\");\n  },\n  methods: {\n    async getOriginalQnaCon() {\n      try {\n        const url = \"/qna/qnaOriginal\";\n        const data = {\n          QNA_ID: this.qna_id\n        };\n        const getOriginalQnaCon = (await axios({\n          method: 'post',\n          url,\n          data\n        }).catch(error => {\n          console.log(error);\n        })).data;\n        if (getOriginalQnaCon.length > 0) {\n          this.getOriginalQnaCon = getOriginalQnaCon;\n        }\n        console.log(this.getOriginalQnaCon);\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async qnaEdit() {\n      const editData = {\n        QNA_TITLE: this.qna_title,\n        QNA_CON: this.qna_con,\n        QNA_ID: this.qna_id\n      };\n      axios.post(\"/qna/qnaEdit\", editData).then(response => {\n        window.location.reload();\n      }).catch(error => {\n        console.error(\"Error deleting Q&A\", error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","GnbBar","components","data","qna","email","qna_title","qna_con","qna_rep","qna_id","created","localStorage","getItem","methods","getOriginalQnaCon","url","QNA_ID","method","catch","error","console","log","length","qnaEdit","editData","QNA_TITLE","QNA_CON","post","then","response","window","location","reload"],"sources":["C:\\Users\\Lim\\Desktop\\BOOK_0728\\MOONMOON\\vue_front\\src\\views\\qnaEdit.vue"],"sourcesContent":["<style src=\"../assets/css/qnaEdit.css\"></style>\r\n<template>\r\n  <div>\r\n    <GnbBar />\r\n\r\n    <div class=\"mypage_menu\"></div>\r\n    <div class=\"right_box\">\r\n      <h1 class=\"qna_title_big\">1:1 문의 수정</h1>\r\n      <div class=\"write_box\">\r\n        <div class=\"title_write\">\r\n          <h2 class=\"title_text\">제목</h2>\r\n          <input class=\"title_input\" type=\"text\" v-model=\"qna.qna_title\" placeholder=\"{{ qna.qna_title }}\">\r\n        </div>\r\n        <div class=\"con_write\">\r\n          <h2 class=\"con_text\">내용</h2>\r\n          <input class=\"con_input\" type=\"text\" v-model=\"qna.qna_con\" placeholder=\"{{ qna.qna_con }}\">\r\n        </div>\r\n      </div>\r\n      <div class=\"btn_box\">\r\n        <button class=\"btn_cancel\">취소</button>\r\n        <button class=\"btn_accept\" @click=\"qnaEdit\">문의 수정</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nimport GnbBar from '../components/gnbBar.vue'\r\n\r\nexport default {\r\n  components: { \r\n    GnbBar \r\n  },\r\n  data() {\r\n    return {\r\n      qna: {\r\n        email: \"\",\r\n        qna_title: \"\",\r\n        qna_con: \"\",\r\n        qna_rep: 0\r\n      },\r\n      qna_id: \"\",\r\n    }\r\n  },\r\n  created() {\r\n    this.qna.email = localStorage.getItem(\"userID\");\r\n  },\r\n  methods: {\r\n    async getOriginalQnaCon() {\r\n      try {\r\n        const url = \"/qna/qnaOriginal\";\r\n        const data = { QNA_ID: this.qna_id };\r\n\r\n        const getOriginalQnaCon = (await axios({\r\n          method: 'post',\r\n          url,\r\n          data\r\n        }).catch(error => {\r\n          console.log(error);\r\n        })).data;\r\n\r\n        if (getOriginalQnaCon.length > 0) {\r\n        this.getOriginalQnaCon = getOriginalQnaCon;\r\n\r\n      }\r\n      console.log(this.getOriginalQnaCon);\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n    async qnaEdit() {\r\n      const editData = {\r\n        QNA_TITLE: this.qna_title,\r\n        QNA_CON: this.qna_con,\r\n        QNA_ID: this.qna_id\r\n      }\r\n\r\n      axios.post(\"/qna/qnaEdit\", editData)\r\n          .then(response => {\r\n            window.location.reload();\r\n          })\r\n          .catch(error => {\r\n            console.error(\"Error deleting Q&A\", error);\r\n          });\r\n    },\r\n  }\r\n  \r\n}\r\n</script>"],"mappings":"AA2BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,OAAOC,MAAK,MAAO,0BAAyB;AAE5C,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE;QACHC,KAAK,EAAE,EAAE;QACTC,SAAS,EAAE,EAAE;QACbC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE;MACX,CAAC;MACDC,MAAM,EAAE;IACV;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACN,GAAG,CAACC,KAAI,GAAIM,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC;EACjD,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,iBAAiBA,CAAA,EAAG;MACxB,IAAI;QACF,MAAMC,GAAE,GAAI,kBAAkB;QAC9B,MAAMZ,IAAG,GAAI;UAAEa,MAAM,EAAE,IAAI,CAACP;QAAO,CAAC;QAEpC,MAAMK,iBAAgB,GAAI,CAAC,MAAMd,KAAK,CAAC;UACrCiB,MAAM,EAAE,MAAM;UACdF,GAAG;UACHZ;QACF,CAAC,CAAC,CAACe,KAAK,CAACC,KAAI,IAAK;UAChBC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;QACpB,CAAC,CAAC,EAAEhB,IAAI;QAER,IAAIW,iBAAiB,CAACQ,MAAK,GAAI,CAAC,EAAE;UAClC,IAAI,CAACR,iBAAgB,GAAIA,iBAAiB;QAE5C;QACAM,OAAO,CAACC,GAAG,CAAC,IAAI,CAACP,iBAAiB,CAAC;MACnC,EAAE,OAAOK,KAAK,EAAE;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB;IACF,CAAC;IACD,MAAMI,OAAOA,CAAA,EAAG;MACd,MAAMC,QAAO,GAAI;QACfC,SAAS,EAAE,IAAI,CAACnB,SAAS;QACzBoB,OAAO,EAAE,IAAI,CAACnB,OAAO;QACrBS,MAAM,EAAE,IAAI,CAACP;MACf;MAEAT,KAAK,CAAC2B,IAAI,CAAC,cAAc,EAAEH,QAAQ,EAC9BI,IAAI,CAACC,QAAO,IAAK;QAChBC,MAAM,CAACC,QAAQ,CAACC,MAAM,CAAC,CAAC;MAC1B,CAAC,EACAd,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,oBAAoB,EAAEA,KAAK,CAAC;MAC5C,CAAC,CAAC;IACR;EACF;AAEF"},"metadata":{},"sourceType":"module","externalDependencies":[]}