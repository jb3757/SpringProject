{"ast":null,"code":"import axios from \"axios\";\naxios.defaults.baseURL = \"http://localhost:3000\";\naxios.defaults.headers.post[\"Content-Type\"] = \"application/json;charset=utf-8\";\naxios.defaults.headers.post[\"Access-Control-Allow-Origin\"] = \"*\";\nimport SideBar from '../components/adminSidebar.vue';\nexport default {\n  name: '',\n  components: {\n    SideBar\n  },\n  data() {\n    return {\n      bookData: [],\n      //검색 키워드\n      search: \"\",\n      //정렬\n      filterValue: \"최신순\",\n      // 페이징\n      perPage: 10,\n      // 한 페이지에 보여줄 게시물 수\n      currentPage: 1,\n      // 현재 페이지 번호\n      maxDisplayedPages: 9 // 표시할 최대 페이지 수 (현재 페이지를 중심으로 좌우로 표시)\n    };\n  },\n\n  setup() {},\n  created() {},\n  mounted() {\n    this.getBookData();\n  },\n  unmounted() {},\n  computed: {\n    totalPages() {\n      return Math.ceil(this.bookData.length / this.perPage);\n    },\n    displayedPosts() {\n      const start = Math.max(0, (this.currentPage - 1) * this.perPage);\n      const end = Math.min(this.bookData.length, this.currentPage * this.perPage);\n      return this.bookData.slice(start, end);\n    },\n    pageNumbers() {\n      const start = Math.max(1, this.currentPage - Math.floor(this.maxDisplayedPages / 2));\n      const end = Math.min(this.totalPages, start + this.maxDisplayedPages - 1);\n      return Array.from({\n        length: end - start + 1\n      }, (_, i) => start + i);\n    }\n  },\n  methods: {\n    async getBookData() {\n      //신상품순\n      if (this.filterValue === \"최신순\") {\n        await axios({\n          url: \"http://localhost:3000/admin/adminBookLatest\",\n          method: \"POST\",\n          data: {}\n        }).then(res => {\n          this.bookData = res.data;\n        }).catch(err => {\n          alert(err);\n        });\n      } else if (this.filterValue === \"판매순\") {\n        await axios({\n          url: \"http://localhost:3000/admin/adminBookSalesPoint\",\n          method: \"POST\",\n          data: {}\n        }).then(res => {\n          this.bookData = res.data;\n        }).catch(err => {\n          alert(err);\n        });\n      }\n    },\n    async deleteBookData(bookId) {\n      //책 삭제\n      await axios({\n        url: \"http://localhost:3000/admin/adminBookDel\",\n        method: \"POST\",\n        data: {\n          book_id: bookId\n        }\n      }).then(res => {\n        alert(\"삭제되었습니다.\");\n        this.getBookData();\n      }).catch(err => {\n        alert(err);\n      });\n    },\n    formatNumber(number) {\n      // 숫자를 천 단위마다 쉼표가 있는 형식으로 변환\n      return new Intl.NumberFormat().format(number);\n    },\n    changePage(pageNumber) {\n      if (pageNumber === 'first') {\n        pageNumber = 1;\n      } else if (pageNumber === 'last') {\n        pageNumber = this.totalPages;\n      } else {\n        pageNumber = parseInt(pageNumber);\n      }\n      window.scrollTo({\n        top: 0,\n        behavior: 'auto'\n      });\n      if (pageNumber >= 1 && pageNumber <= this.totalPages) {\n        this.currentPage = pageNumber;\n      }\n    },\n    async getSearchKeyword(event) {\n      //검색 함수\n      this.search = event.target.value;\n      console.log(this.search);\n      await axios({\n        url: \"http://localhost:3000/main/new\",\n        method: \"GET\",\n        params: {\n          searchKeyword: this.search\n        }\n      }).then(res => {\n        console.log(res.data);\n        this.bookData = res.data;\n      }).catch(err => {\n        alert(err);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","defaults","baseURL","headers","post","SideBar","name","components","data","bookData","search","filterValue","perPage","currentPage","maxDisplayedPages","setup","created","mounted","getBookData","unmounted","computed","totalPages","Math","ceil","length","displayedPosts","start","max","end","min","slice","pageNumbers","floor","Array","from","_","i","methods","url","method","then","res","catch","err","alert","deleteBookData","bookId","book_id","formatN